{
  "name": "terra-react-testing-library",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-intl": "^2.9.0",
    "react-scripts": "3.2.0",
    "terra-aggregate-translations": "^1.3.0",
    "terra-base": "^5.21.0",
    "terra-button": "^3.25.0"
  },
  "scripts": {
    "aggregate-translations": "tt-aggregate-translations -b ./ -d ./src/**/translations -d ./translations -o ./node_modules/terra-i18n/node_modules -f es6",
